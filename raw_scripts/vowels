setcpm(80)
stack(
  n(`
    <<0 2 4>*4(3,8,0)>
  `).fm(40)
  .fmattack("<0 .05 .1 .2>").vowel("[a|e|i|o|u]*4").clip(4).room(2).lpf(sine.range(100, 2000)),
  note(`<
    [0 2 0 4]
    [0 2 1 4]
    [0 2@1.5 [5 1] 4@0.5]
    [0 2 1@0.5 4@1.5]
  >`)
  .scale("<C4:major A4:minor>/3")
  .fm(4)
  .fmattack("<.05 .1>/2")
  .room(2).lpf(800).gain("<1 1.5>*4")
)
