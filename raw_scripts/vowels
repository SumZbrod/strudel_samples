setcpm(60)
stack(
  n(`
    <<0 2 4>*4(3,8,0)>
  `).fm(40)
  .fmattack("<0 .05 .1 .2>").vowel("[a|e|i|o|u]*4").clip(4).room(2).lpf(sine.range(100, 1000)),
  note(`<
    [0 2 0 4]
    [0 1 1 4]
    [0 2 [5 1] 4]
    [0 1 4@2]
  >`)
  .scale("<C4:major A4:minor>/3")
  .fm("<4 5>*4")
  .fmattack("<.05 .1>/2")
  .room(2).lpf(500).gain("<1 1.25>*4")
)
